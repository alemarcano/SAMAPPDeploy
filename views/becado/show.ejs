<div class="my-4">
    <div class="container text-center white-text">
    <div class="container text-center">
     <h1 class="my-4">Estudiantes Becados:</h1>
      <table class="table white-text">
        <tr>
          <th>Cedula</th>
          <th>Nombre</th>
          <th>Condicion</th>
          <th>Tipo de Beca</th>


         
  
        </tr>
        <% becados.forEach( function(becado){ %>
          <tr>
              <td><%= becado.Cedula %></td>
              <td><%= becado.NombreCompleto %></td>
              <td><%= becado.condicion %></td>
              <td><%= becado.tipo %></td>

            </tr>
        <% }) %>
      </table>
    </div>
  </div>
