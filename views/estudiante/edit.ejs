<div class="d-flex align-items-center justify-content-center w-100 h-100">
        <div class="text-center white-text my-auto align-middle">
            <div>
              <h3 class="p-3 mb-2"> Editar un Estuddiante:</h3>
    
                <form action="<%= '/editEstudiante/?id='+estudiante.idEstudiante %>" method="POST">
                <table class="table">
                  <tr>
                    <th>Carnet:</th>
                    <td><input class="form-control"  type="text" name="carnet" required value="<%= estudiante.carnet %>"></td>
                  </tr>
                  <tr>
                    <th>Persona:</th>
                    <td>
                      <select name="persona" class="form-control">
                          <option value="<%= profesor.persona.idPersona %>" selected> <%= estudiante.persona.nombres + ' ' + estudiante.persona.apellidos %> </option>
                        <% personas.forEach(function(persona){ %>
                          <option value="<%= persona.idPersona %>" > <%= persona.Nombres + ' ' + persona.Apellidos %> </option>
                        <% }) %>
                      </select>
                    </td>
                  </tr>
                  <tr>
                    
                  </tr>
                  <tr>
                    <td  colspan="6"> <input class="btn btn-success w-50" type="submit" value="Submit"> </td>
                  </tr>
                </table>
              </div>
            </form>
          </div>
      </div>